{
  "name": "bacon-lorem",
  "main": "./lib/bacon-lorem",
  "version": "0.1.0",
  "description": "Atom package that let's you automatically insert \"bacon ipsum\" dummy/placeholder text to any file in atom.",
  "keywords": [
    "lorem",
    "bacon",
    "dummy"
    "generator",
    "placeholder"
    "pork",
    "beaf",
    "meat",
    "meatball"
  ],
  "activationCommands": {
    "atom-workspace": "bacon-lorem:toggle"
  },
  "repository": "https://github.com/atom/bacon-lorem",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "axios": "^0.15.3"
  }
}
